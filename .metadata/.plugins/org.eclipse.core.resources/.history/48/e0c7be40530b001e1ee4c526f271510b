
public class ArithOp {

	public static void main(String[] args) {
		int num1 = 7;
		int num2 = 3;
		//대입 연산자와 산술 연산자의 예
		num1 = num1 + 7;
		System.out.println(num1);
		
		System.out.println("num1 + num2 = " + (num1 + num2));
		System.out.println("num1 - num2 = " + (num1 - num2));
		System.out.println("num1 * num2 = " + (num1 * num2));
		System.out.println("num1 / num2 = " + (num1 / num2));
		System.out.println("num1 % num2 = " + (num1 % num2));
	}

}
